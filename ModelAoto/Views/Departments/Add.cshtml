@model ModelAoto.Models.Entities.EmployeeDepartment
@{
    ViewBag.Title = "Add";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br>
<h2>Yeni Departman</h2>
<br>

<form class="form-group" method="post">

    @Html.LabelFor(x => x.DepartmentName)
    @Html.TextBoxFor(x => x.DepartmentName, new { @class = "form-control" })
    <br />

    @Html.LabelFor(x => x.Status)
    @Html.CheckBoxFor(x => x.Status)
    <br />



    <button class="btn btn-outline-primary">Ekle</button>
</form>

